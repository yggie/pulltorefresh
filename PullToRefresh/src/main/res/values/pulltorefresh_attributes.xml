<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PullListFragment">
        <attr name="list_backgroundColor" format="color"/>
        <attr name="list_padding" format="dimension"/>
        <attr name="list_paddingTop" format="dimension"/>
        <attr name="list_paddingBottom" format="dimension"/>
        <attr name="list_paddingLeft" format="dimension"/>
        <attr name="list_paddingRight" format="dimension"/>
        <attr name="top_mode" format="enum">
            <enum name="none" value="0"/>
            <enum name="pull" value="1"/>
        </attr>
        <attr name="bottom_mode" format="enum">
            <enum name="none" value="0"/>
            <enum name="pull" value="1"/>
        </attr>
        <attr name="top_view" format="reference"/>
        <attr name="bottom_view" format="reference"/>
        <attr name="top_pullStartedText" format="string"/>
        <attr name="top_pullThresholdText" format="string"/>
        <attr name="top_refreshingText" format="string"/>
        <attr name="top_refreshCompleteText" format="string"/>
        <attr name="bottom_pullStartedText" format="string"/>
        <attr name="bottom_pullThresholdText" format="string"/>
        <attr name="bottom_refreshingText" format="string"/>
        <attr name="bottom_refreshCompleteText" format="string"/>
        <attr name="empty_text" format="string"/>
        <attr name="empty_view" format="reference"/>
        <attr name="top_backgroundColor" format="color"/>
        <attr name="top_innerBackgroundColor" format="color"/>
        <attr name="bottom_backgroundColor" format="color"/>
        <attr name="bottom_innerBackgroundColor" format="color"/>
        <attr name="pull_delay" format="integer"/>
        <attr name="pull_damping" format="float"/>
        <attr name="release_easing" format="float"/>
        <attr name="top_textColor" format="color"/>
        <attr name="top_textSize" format="dimension"/>
        <attr name="bottom_textColor" format="color"/>
        <attr name="bottom_textSize" format="dimension"/>
    </declare-styleable>
</resources>